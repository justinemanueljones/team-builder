,{"generated":{"line":117,"column":17},"source":"components/App.js","original":{"line":73,"column":31}},{"name":"id","generated":{"line":117,"column":18},"source":"components/App.js","original":{"line":73,"column":32}},{"generated":{"line":117,"column":20},"source":"components/App.js","original":{"line":73,"column":12}},{"generated":{"line":118,"column":0},"source":"components/App.js","original":{"line":73,"column":36}},{"generated":{"line":118,"column":6},"source":"components/App.js","original":{"line":73,"column":36}},{"generated":{"line":118,"column":13},"source":"components/App.js","original":{"line":73,"column":43}},{"name":"friend","generated":{"line":118,"column":15},"source":"components/App.js","original":{"line":73,"column":45}},{"generated":{"line":119,"column":0},"source":"components/App.js","original":{"line":73,"column":12}},{"generated":{"line":119,"column":6},"source":"components/App.js","original":{"line":72,"column":10}},{"generated":{"line":120,"column":0},"source":"components/App.js","original":{"line":75,"column":9}},{"generated":{"line":120,"column":3},"source":"components/App.js","original":{"line":71,"column":8}},{"generated":{"line":120,"column":4},"source":"components/App.js","original":{"line":58,"column":4}},{"generated":{"line":120,"column":5},"source":"components/App.js","original":{"line":57,"column":2}},{"generated":{"line":121,"column":0},"source":"components/App.js","original":{"line":79,"column":1}}],"sources":{"components/App.js":"import React, { useState, useEffect } from 'react'\nimport Friend from './Friend'\nimport FriendForm from './FriendForm'\nimport axios from '../axios'\n\n// ðŸ‘‰ the shape of the state that drives the form\nconst initialFormValues = {\n  ///// TEXT INPUTS /////\n  username: '',\n  email: '',\n  ///// DROPDOWN /////\n  role: '',\n}\n\nexport default function App() {\n  const [friends, setFriends] = useState([]) // careful what you initialize your state to\n\n  // ðŸ”¥ STEP 1 - WE NEED STATE TO HOLD ALL VALUES OF THE FORM!\n  const [formValues, setFormValues] = useState(initialFormValues) // fix this using the state hook\n\n  const updateForm = (inputName, inputValue) => {\n    // ðŸ”¥ STEP 8 - IMPLEMENT a \"form state updater\" which will be used inside the inputs' `onChange` handler\n    //  It takes in the name of an input and its value, and updates `formValues`\n    setFormValues({ ...formValues, [inputName]: inputValue })\n  }\n\n  const submitForm = () => {\n    // ðŸ”¥ STEP 9 - IMPLEMENT a submit function which will be used inside the form's own `onSubmit`\n    //  a) make a new friend object, trimming whitespace from username and email\n      const newFriend = {\n        username: formValues.username.trim(),\n        email: formValues.email.trim(),\n        role: formValues.role,\n      }\n    //  b) prevent further action if either username or email or role is empty string after trimming\n    if (!newFriend.username || !newFriend.email || !newFriend.role) {\n      return\n    }\n    //  c) POST new friend to backend, and on success update the list of friends in state with the new friend from API\n    //  d) also on success clear the form\n    axios.post('fakeapi.com', newFriend)\n      .then(res => {\n        // setFriends(friends.concat(res.data))\n        setFriends([ res.data, ...friends])\n        setFormValues(initialFormValues)\n      })\n      .catch(err => {\n        debugger\n        // things like backend rejecting the POST because duplicate username etc\n      })\n  }\n\n  useEffect(() => {\n    axios.get('fakeapi.com').then(res => setFriends(res.data))\n  }, [])\n\n  return (\n    <div className='container'>\n      <h1>Unit 2 Form Management</h1>\n\n      <FriendForm\n        // ðŸ”¥ STEP 2 - The form component needs its props.\n        //  Check implementation of FriendForm\n        //  to see what props it expects.\n        values={formValues}\n        update={updateForm}\n        submit={submitForm}\n      />\n\n      {\n        friends.map(friend => {\n          return (\n            <Friend key={friend.id} details={friend} />\n          )\n        })\n      }\n    </div>\n  )\n}\n"},"lineCount":null}},"error":null,"hash":"9db138b12c7ff923f0299438dba26083","cacheData":{"env":{}}}